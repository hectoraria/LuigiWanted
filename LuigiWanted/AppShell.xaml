<?xml version="1.0" encoding="UTF-8"?>

<Shell
    x:Class="LuigiWanted.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:LuigiWanted"
    Shell.FlyoutBehavior="Disabled"
    Title="LuigiWanted"
    xmlns:views="clr-namespace:LuigiWanted.Pages">

    <ShellContent Title="Wanted" Route="Wanted" ContentTemplate="{DataTemplate views:Wanted}"></ShellContent>

    <ShellContent Title="Register" Route="Register" ContentTemplate="{DataTemplate views:MainPage}"></ShellContent>

    

    <ShellContent Title="Buscar" Route="Buscar" ContentTemplate="{DataTemplate views:Buscar}"></ShellContent>
</Shell>